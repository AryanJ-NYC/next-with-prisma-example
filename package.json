  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@apollo/react-ssr": "^3.1.4",
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
